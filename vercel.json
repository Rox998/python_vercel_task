{
    "builds": [
      {
        "src": "carpeta_de_proyecto/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.10"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "carpeta_de_proyecto/wsgi.py"
      }
    ]
  }
   